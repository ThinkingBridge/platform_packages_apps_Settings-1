<preference-headers
        xmlns:android="http://schemas.android.com/apk/res/android">


    <!-- UserInterface Headers -->
    <header
        android:title="@string/user_interface" />

    <!--<header
        android:id="@+id/general_ui"
        android:title="@string/general_ui"
        android:icon="@drawable/ic_settings_general_ui"
        android:fragment="com.android.settings." />-->

    <header
        android:id="@+id/statusbar"
        android:title="@string/status_bar_title"
        android:icon="@drawable/ic_settings_statusbar"
        android:fragment="com.android.settings.thinkingbridge.StatusBar" />

    <header
        android:id="@+id/powerwidget"
        android:title="@string/notification_drawer_title"
        android:icon="@drawable/ic_settings_powerwidget"
        android:fragment="com.android.settings.cyanogenmod.PowerWidget" />

    <header
        android:id="@+id/quicksettings"
        android:title="@string/quick_settings_panel_title"
        android:icon="@drawable/ic_settings_quicksettings"
        android:fragment="com.android.settings.thinkingbridge.QuickSettings" />

    <header
        android:id="@+id/power_menu"
        android:title="@string/power_menu_title"
        android:icon="@drawable/ic_settings_powermenu"
        android:fragment="com.android.settings.thinkingbridge.PowerMenu" />

    <!-- Navbar headers -->

    <header
        android:title="@string/navbar_headers" />

    <header
        android:id="@+id/pie_control"
        android:title="@string/title_pie"
        android:icon="@drawable/ic_settings_pie"
        android:fragment="com.android.settings.thinkingbridge.Pie" />

</preference-headers>